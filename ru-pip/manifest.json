{
    "name": "RU PiP",
    "description": "Picture in Picture Extension",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        }
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+P"
            }
        }
    },
    "icons": {
        "128": "images/icon128.png"
    },
    "minimum_chrome_version": "69.0.3483.0"
}